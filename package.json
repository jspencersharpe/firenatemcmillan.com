    "@angular/platform-browser": "4.0.3",
    "@angular/platform-browser-dynamic": "4.0.3",
    "@angular/router": "4.0.3",
    "angulartics2": "^2.0.0",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "express": "^4.15.2",
    "lodash": "^4.17.4",
    "ngx-bootstrap": "^1.6.6",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.1",
    "topojson": "^2.2.0",
    "zone.js": "0.8.5"
